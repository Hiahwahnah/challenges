    "# CONVERT RADIANS TO DEGREES\n",
    "\n",
    "# import math library\n",
    "import math\n",
    "\n",
    "# get the input\n",
    "radians = float(input(\"Input the radians please: \"))\n",
    "\n",
    "# calculate conversion\n",
    "degrees =  radians*(180/(math.pi))\n",
    "\n",
    "# print results\n",
    "print(str(radians) + \" radians converts to \" + str(degrees) + \" degrees\")"
   ]
